generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id           Int       @id @default(autoincrement())
  email        String
  supertokenId String
  name         String?
  cpf          String?
  job          String?
  createdAt    DateTime?
  score        Int
  profile      String?
}

model newspaper {
  id          String   @unique
  source      Json?
  author      String
  title       String
  description String?
  url         String
  urlToImage  String?
  publishedAt DateTime
  content     String?
}
